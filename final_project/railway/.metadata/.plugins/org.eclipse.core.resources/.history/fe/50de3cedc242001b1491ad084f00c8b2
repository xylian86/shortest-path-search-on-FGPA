typedef struct Open_list Open_list, * pOpen_list;
typedef struct Node
{
    //çˆ¶èŠ‚ç‚?
    pOpen_list  pFather;
    int   N;
    float G;
    float H;
    //Få€?
    float F;
    //x,yåæ ‡
    float x;
    float y;
}Node, * pNode;

typedef struct Open_list
{
    struct Open_list * next;
    struct Node node;
}Open_list, * pOpen_list;

//---------------------------------------------------------------------------//
//ç›¸å…³å‡½æ•°
float my_abs(float x);
//è·ç¦»å‡½æ•°ï¼ˆç»™ä¸¤ä¸ªåæ ‡(x1,y1)å’Œï¼ˆx2ï¼Œy2ï¼‰ï¼‰
float my_distance(int x1, int y1, int x2, int y2);
//æ·»åŠ é“¾è¡¨
void list_add_tail(pOpen_list my_list, pOpen_list add_node);
//éåŽ†é“¾è¡¨,è¿”å›žä¸ŽtmpYå’ŒtmpYåŒ¹é…çš„èŠ‚ç‚¹æŒ‡é’?
pOpen_list list_browse(pOpen_list my_list, int N);
//åˆ¤æ–­é“¾è¡¨ä¸­æ˜¯ä¸æ˜¯æœ‰æŸä¸ªèŠ‚ç‚¹ï¼ˆé€šè¿‡åæ ‡æ¥ç¡®å®šï¼‰ï¼Œæœ‰çš„è¯è¿”å›ž0ï¼Œæ²¡æœ‰è¿”å›?
int judge_node_exist(pOpen_list mylist, int N);
//åˆ é™¤é“¾è¡¨ä¸­æŸä¸ªèŠ‚ç‚¹ï¼Œé€šè¿‡åæ ‡åˆ é™¤,å¹¶ä¸”è¿”å›žè¿™ä¸ªåˆ é™¤çš„èŠ‚ç‚¹æŒ‡é’ˆï¼Œæ–¹ä¾¿åŠ å…¥åˆ°close list
pOpen_list list_delete_point(pOpen_list my_list, int N);
//æ‰¾åˆ°é“¾è¡¨ä¸­æœ€å°çš„få€¼çš„å‡½æ•°,è¾“å…¥é“¾è¡¨ï¼Œè¿”å›žæœ€å°fçš„èŠ‚ç‚?
pOpen_list find_min_f(pOpen_list my_list);
//æ‰“å°open listä¸­çš„å„ä¸ªèŠ‚ç‚¹åæ ‡ä»¥åŠFå€?
